{{ form_start(form) }}
    <div class="form-group">
        {{ form_row(form.type) }}
    </div>

     <div class="form-group">
        {{ form_row(form.name) }}
    </div>
     <div class="form-group">
        {{ form_row(form.email) }}
    </div>
     <div class="form-group">
        {{ form_row(form.head_office_address) }}
    </div>

     <div class="form-group">
        {{ form_row(form.zipcode) }}
    </div>

     <div class="form-group">
        {{ form_row(form.city) }}
    </div>

     <div class="form-group">
        {{ form_row(form.rcs_city) }}
    </div>
    <div class="form-group">
        {{ form_row(form.phone) }}
    </div>
    <button class="btn btn-sm btn-primary">{{ button_label|default('Ajouter') }}</button>
{{ form_end(form) }}